NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Set to "true" to use in-memory store (no PostgreSQL needed)
USE_IN_MEMORY=true

# PostgreSQL (only used when USE_IN_MEMORY=false)
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=task_management
DB_SSL=false
DB_POOL_MIN=2
DB_POOL_MAX=10
